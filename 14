public class Control {
    public static void main(String[] args) {
        int x = 5;
        if (x > 0)
            System.out.println("Positive");
        
        switch (x) {
            case 5: System.out.println("Five"); break;
            default: System.out.println("Other");
        }

        for (int i = 0; i < 3; i++)
            System.out.println(i);
    }
}
